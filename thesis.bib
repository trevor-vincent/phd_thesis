  language	= {English},
  keywords	= {Discontinuous Galerkin Finite-Element Method, Model,
		  Water-Waves, High-Order Boussinesq-Type Equations,
		  Propagation, Form, Unstructured Grids, Shallow-Water,
		  Finite-Element-Method, Stability, Gravity Waves},
  date-added	= {2008-12-23 11:53:10 -0500},
