@Article{	  hesthaven:2008p32,
  author	= {Jan S Hesthaven and Robert M Kirby},
  journal	= {Math. Comp.},
  title		= {Filtering in Legendre spectral methods},
  abstract	= {We discuss the impact of modal filtering in Legendre
		  spectral methods, both on accuracy and stability. For the
		  former, we derive sufficient conditions on the filter to
		  recover high order accuracy away from points of
		  discontinuity. Computational results confirm that less
		  strict necessary conditions appear to be adequate. We
		  proceed to discuss a instability mechanism in polynomial
		  spectral methods and prove that filtering suffices to
		  ensure stability. The results are illustrated by
		  computational experiments.},
