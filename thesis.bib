  journal	= {Numerical Linear Algebra with Applications},
  number	= {9},
  owner		= {Yunrong Zhu},
  pages		= {753--770},
  timestamp	= {2008.07.23},
  title		= {Two-level preconditioning of discontinuous Galerkin
